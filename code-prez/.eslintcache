 } from 'react'\nimport styled, { css } from 'styled-components'\nimport RecorderNavButton from '../RecorderNavButton' \nimport Record from '../../assets/record.png'\nimport Pause from '../../assets/pause.png'\nimport Stop from '../../assets/stop.png'\nimport { RecordingStates } from '../../pages/Record'\nimport './animation.css'\n\nconst RecorderNavContainer = styled.div`\n    width:100%;\n    height:110px;\n    background-color:#313C4E;\n    display:flex;\n    justify-content:center;\n`\nconst RecorderNav = styled.div`\n    width:80%;\n    height:100%;\n    display:flex;\n    justify-content:flex-start;\n    align-items:center;\n`\nconst RecorderNavButtons = styled.div`\n    display:flex;\n    justify-content:flex-start;\n    align-items:center;\n`\nconst RecorderNavTimer = styled.div`\n    margin-left:50px;\n    font-size:2rem;\n    color:#ffffff;\n`\n\n\nexport default function CodeRecordNav(props){\n\n    return (\n        <RecorderNavContainer>\n            <RecorderNav>\n                <RecorderNavButtons>\n                    <RecorderNavButton disabled={props.recording === RecordingStates.RECORDING || props.recording === RecordingStates.PAUSED} onClick={props.record} ><img src={Record} alt=\"\" className={props.recording === RecordingStates.RECORDING ? 'blink' : ''} /></RecorderNavButton>\n                    <RecorderNavButton disabled={props.recording === RecordingStates.NOT_STARTED || props.recording === RecordingStates.FINISHED} onClick={props.pause}><img src={Pause} alt=\"\" /></RecorderNavButton>\n                    <RecorderNavButton disabled={props.recording === RecordingStates.NOT_STARTED || props.recording === RecordingStates.FINISHED} onClick={props.stop}><img src={Stop} alt=\"\" /></RecorderNavButton>\n                </RecorderNavButtons>\n                <RecorderNavTimer>\n                    {props.mins.toString().length === 1 && '0'}{props.mins }:{props.seconds.toString().length === 1 && '0'}{props.seconds}\n                </RecorderNavTimer>\n            </RecorderNav>\n        </RecorderNavContainer>\n    )\n}","/Users/macjohnalds/Desktop/hackthenorth/code-prez/src/components/RecorderNavButton/index.js",[],"/Users/macjohnalds/Desktop/hackthenorth/code-prez/src/helpers/timer.js",[],"/Users/macjohnalds/Desktop/hackthenorth/code-prez/src/hooks/useRecorder.js",["84"],"//Follow the pattern below to use the useRecorder hook\n\n\n// const playback = (url) => {\n//     const audio = new Audio(url);\n//     audio.play();\n//   };\n\n//   const recorder = useRecorder(playback);\n\n//   return (\n//     <div className=\"App\">\n//       <button onClick={recorder.start}>Record</button>\n//       <button onClick={recorder.stop}>stop</button>\n//       <button onClick={recorder.pause}>Pause</button>\n//     </div>\n//   );\n\n\n\nimport { useEffect, useState } from \"react\";\nexport default function useRecorder(getNewTrack) {\n  const [mediaRecorder, setMediaRecorder] = useState();\n\n  useEffect(() => {\n    navigator.mediaDevices\n      .getUserMedia({ audio: true })\n      .then((stream) => setMediaRecorder(new MediaRecorder(stream)))\n      .catch((e) => {\n        console.log(e);\n      });\n\n    return setMediaRecorder();\n  }, [navigator]);\n\n  const startRecording = () => {\n    if (mediaRecorder) {\n      mediaRecorder.start();\n      const audioChunks = [];\n      mediaRecorder.addEventListener(\"dataavailable\", (event) => {\n        audioChunks.push(event.data);\n      });\n\n      mediaRecorder.addEventListener(\"stop\", () => {\n        const audioBlob = new Blob(audioChunks);\n        getNewTrack(URL.createObjectURL(audioBlob));\n      });\n      \n    }\n  };\n\n  const stopRecording = () => {\n    if (mediaRecorder) {\n      mediaRecorder.stop();\n    }\n  };\n\n  const togglePause = () => {\n    if (mediaRecorder) {\n      if (mediaRecorder.state === \"paused\") {\n        mediaRecorder.resume();\n      } else if (mediaRecorder.state === \"recording\") {\n        mediaRecorder.stop();\n        mediaRecorder.start();\n        mediaRecorder.pause();\n      }\n    }\n  };\n\n  return {\n    start: startRecording,\n    stop: stopRecording,\n    pause: togglePause,\n    mediaRecorder\n  };\n}\n","/Users/macjohnalds/Desktop/hackthenorth/code-prez/src/components/PreviewButton/index.js",["85"],"import React from 'react';\nimport styled from 'styled-components';\n\nimport play from '../../assets/play.png';\nimport backward from '../../assets/backward.png';\nimport forward from '../../assets/forward.png'\nconst IMAGES = [\n    {type: \"play\", src: play},\n    {type: \"backward\", src: backward},\n    {type: \"forward\", src: forward}\n];\n\nexport default function PreviewButton({ details }) {\n\n    const buttonImg = IMAGES\n    .filter(img => img.type === details.type);\n    console.log(buttonImg[0].src);\n    return (\n        <Button \n            onClick={() => {details.action()}}\n        >\n            <img style={{height: \"15px\", width: \"15px\"}}src={buttonImg[0].src} />\n        </Button>\n    )\n}\n\nconst Button = styled.button`\n    background-color: #313C4E;\n    width: 70px;\n    height: 35px;\n    border: none;\n    border-radius: 3px;\n    cursor: pointer;\n    display: flex;\n    justify-content: center;\n    align-items: center;\n`;","/Users/macjohnalds/Desktop/hackthenorth/code-prez/src/components/PreviewSpace/index.js",["86"],"/Users/macjohnalds/Desktop/hackthenorth/code-prez/src/components/CodeSpace/index.js",["87","88"],"import React, { useEffect, useState } from 'react';\nimport styled from 'styled-components';\nimport CodeEditor from '../CodeEditor'\nimport hash from '../../helpers/hash'\n\nexport default function CodeSpace(props) {\n\n    const [content , setContent] = useState('')\n\n    useEffect(() => {\n\n            const timestamp = (props.mins * 60) + props.seconds\n\n            const snapshot = {\n                text : content,\n                timestamp,\n                hash : hash(content)\n            }\n\n            